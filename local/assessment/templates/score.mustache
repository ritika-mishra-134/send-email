
<div>
    <h2>{{#str}}comb, local_assessment{{/str}}
     </h2>
</div>

  <div class="row justify-content-around">
    <div id="chart" class="col-sm-12 col-md-4 card m-10"></div>
    <div id="bar" class="col-sm-12 col-md-7 card m-10"></div>
</div>
<div>
  <h2>{{#str}}sec, local_assessment{{/str}}</h2>
</div>


{{#sectiondata}}
  <div class="row justify-content-around">
    <div class="sectionchart-{{id}} card col-sm-12 col-md-5 m-10" id={{id}}></div>
    <div class="col-sm-12 col-md-6 m-10">
      <table class="table mt-0">
        <thead class="thead-dark">
          <tr>
            <th colspan="4"><h4 style="color: black"><b>{{name}}</b></h4></th>   
          </tr>
        </thead>
        <tbody>
          <tr>
            <th><b>{{#str}}score, local_assessment{{/str}}</b></th> 
            <th><b>{{#str}}correct, local_assessment{{/str}}</b></th>
            <th><b>{{#str}}wrong, local_assessment{{/str}}</b></th>
            <th><b>{{#str}}unatmpt, local_assessment{{/str}}</b></th>    
          </tr>
          <tr>
            <td>{{score}}</td>
            <td>{{correct}}</th>
            <td>{{wrong}}</td>
            <td>{{noattempt}}</td>
            
          </tr>
        </tbody>
      </table>

    </div>
  </div>
{{/sectiondata}}

